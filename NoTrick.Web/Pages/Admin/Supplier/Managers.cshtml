@page "{id}"
@model NoTrick.Web.Pages.Admin.Supplier.Managers

<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Managers</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-page="/Admin/Supplier/Managers">Suppliers</a></li>
                    <li class="breadcrumb-item"><a asp-page="/Admin/Supplier/Edit" asp-route-id="@Model.supplier.Id">@Model.supplier.CompanyName</a></li>
                    <li class="breadcrumb-item active">Managers</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<div class="content">
    <div class="container-fluid">
        <div class="card">
            <div class="card-body">
                <table class="table table-borderless">
                    <tr>
                        <th>Manager Id</th>
                        <th>Manager Name</th>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
  
<!-- /.main-content -->
