@page
@model NoTrick.Web.Pages.Admin.Supplier.Index

@{
    ViewData["Title"] = "Home page";
}
<!-- Content Header (Page header) -->
<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0 text-dark">Supplier Page</h1>
      </div><!-- /.col -->
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a asp-page="/Admin/Supplier/Index">Suppliers</a></li>
          <li class="breadcrumb-item active">Suppliers</li>
        </ol>
      </div><!-- /.col -->
    </div><!-- /.row -->
  </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<div class="content">
  <div class="container-fluid">
    <div class="card">
      <div class="card-body">
        <table class="table table-borderless">
          <tr>
            <th>Supplier Id</th>
            <th>Company Name</th>
            <th>Balance</th>
            <th>Actions</th>
          </tr>
        </table> 
      </div>
    </div>
  </div>
</div>
  
<!-- /.main-content -->
